{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/gateway/src",
  "monorepo": true,
  "root": "apps/gateway",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/gateway/tsconfig.app.json"
  },
  "projects": {
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "experience": {
      "type": "application",
      "root": "apps/experience",
      "entryFile": "main",
      "sourceRoot": "apps/experience/src",
      "compilerOptions": {
        "tsConfigPath": "apps/experience/tsconfig.app.json"
      }
    },
    "company": {
      "type": "application",
      "root": "apps/company",
      "entryFile": "main",
      "sourceRoot": "apps/company/src",
      "compilerOptions": {
        "tsConfigPath": "apps/company/tsconfig.app.json"
      }
    },
    "technology": {
      "type": "application",
      "root": "apps/technology",
      "entryFile": "main",
      "sourceRoot": "apps/technology/src",
      "compilerOptions": {
        "tsConfigPath": "apps/technology/tsconfig.app.json"
      }
    }
  }
}